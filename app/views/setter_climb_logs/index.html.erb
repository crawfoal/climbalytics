<h1>Listing SetterClimbLogs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Grade</th>
      <th>Picture</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @setter_climb_logs.each do |setter_climb_log| %>
      <tr>
        <td><%= setter_climb_log.picture %></td>
        <td><%= link_to 'Show', setter_climb_log %></td>
        <td><%= link_to 'Edit', edit_setter_climb_log_path(setter_climb_log) if policy(setter_climb_log).update? %></td>
        <td><%= link_to 'Destroy', setter_climb_log, method: :delete, data: { confirm: 'Are you sure?' } if policy(setter_climb_log).update? %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New SetterClimbLog', new_setter_climb_log_path if policy(SetterClimbLog).new? %>
